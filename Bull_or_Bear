#include <stdio.h>

int main(void) {
	// your code goes here
	int t;
	scanf("%d",&t);
	while(t--){
	    int x, y;
	    scanf("%d %d", &x, &y);
	    if(x>y){
	        printf("loss\n");
	    }else if(x==y){
	        printf("neutral\n");
	    }else{
	        printf("profit\n");
	    }
	}
	return 0;
}

