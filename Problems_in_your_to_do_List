#include <stdio.h>
int main(void){
    int t;
    scanf("%d", &t);
    while(t--){
       int n, d[10000], count=0;
       scanf("%d",&n);
       for(int i=0; i<n; i++){
         scanf("%d", &d[i]);
         if(d[i] >= 1000)
         count++;
       }
       printf("%d\n", count);
    }
	return 0;
}


