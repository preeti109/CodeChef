#include <stdio.h>

int main(void) {
	// your code goes here
	int t;
	scanf("%d", &t);
	while(t--){
	    int  w, x, y, z;
	    scanf("%d %d %d %d", &w, &x, &y, &z);
	    if(w-y < x-z){
	        printf("first\n");
	    }else if(w-y == x-z){
	        printf("any\n");
	    }else{
	        printf("second\n");
	    }
	}
	return 0;
}

