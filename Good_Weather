#include <stdio.h>
int main() {
	int t;
	scanf("%d", &t);
	while(t--){
	    int arr[7], i, count1=0, count0=0;
	    for(int i=0; i<7; i++){
	       scanf("%d", &arr[i]);
	    }
	    for(int i=0; i<7; i++){
	       if(arr[i]==1)
	           count1++;
	       else
	           count0++;
	    }
	    if(count1>count0)
	       printf("yes\n");
	    else
	       printf("no\n");
	}
	return 0;
}
