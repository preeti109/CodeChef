#include <stdio.h>
#include <string.h>

int main(){
    int t, count = 0;
    scanf("%d", &t);
    while(t--){
        char x[100];
        scanf("%s", x);
        for(int i = 0; x[i]!='\0'; i++){
            if(x[i]=='7'){
                count++;
                break;
            }
        }
        if(count >= 1)
            printf("Yes\n");
        else
            printf("No\n");
    }
	return 0;
}
