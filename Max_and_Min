#include <stdio.h>
int main(void){
	int t;
	scanf("%d", &t);
	while(t--){
	    int a,b,c;
	    scanf("%d %d %d", &a, &b, &c);
	    if (a>b && a>c){
	        if(b<c){
	            printf("%d\n", (a-b));
	        }else{
	            printf("%d\n", (a-c));
	        }
	    }
	    else if(b>a && b>c){
	        if(a<c){
	            printf("%d\n", (b-a));
	        }else{
	            printf("%d\n", (b-c));
	        }
	    }
	    else if(c>a && c>b){
	        if(a<b){
	            printf("%d\n", (c-a));
	        }else{
	            printf("%d\n", (c-b));
	        }   
	    }
   }
   return 0;
}

