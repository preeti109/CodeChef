#include <stdio.h>
int main(void) {
	int t, i, j;
	scanf("%d", &t);
	while(t--){
	    char arr1[5], arr2[5], arr3[5];
	    scanf("%s %s", &arr1[i], &arr2[j]);
	    for(int i=0; i<5; i++){
	       if(arr1[i]==arr2[i])
	       printf("G");
	       else 
	       printf("B");
	    }
	    printf("\n");
	}
	return 0;
}

