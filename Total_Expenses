import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef {
	public static void main (String[] args) throws java.lang.Exception {
	    Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t-->0){
		    int a = sc.nextInt();
		    int b = sc.nextInt();
		    if(a>1000) {
		        System.out.println((a*b) - (a*b*0.1));
		    }
		    else {
		        System.out.println(a*b);
		    }
		}
	}
}
