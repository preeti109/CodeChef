import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef {
	public static void main (String[] args) throws java.lang.Exception {
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		for(int i=0; i<t; i++) {
		    int g = sc.nextInt();
		    int c = sc.nextInt();
		    System.out.println((c*c)/(2*g));
		}
	}
}
