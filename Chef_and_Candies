#include <stdio.h>

int main(void) {
	// your code goes here
	int t, a, b, c, d;
	scanf("%d", &t);
	while(t--){
	    scanf("%d %d", &a, &b);
	    if(a>=b && (a-b)%4==0)
	    printf("%d\n", (a-b)/4);
	    else if(a>=b && (a-b)%4!=0)
	    printf("%d\n", ((a-b)/4)+1);
	    else
	    printf("0\n");
	}
	return 0;
}

