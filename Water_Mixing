#include <stdio.h>

int main(void) {
	// your code goes here
	int t, a, b, c, d;
	scanf("%d", &t);
	while(t--){
	    scanf("%d %d %d %d", &a, &b, &c, &d);
	    if(a == b)
	        printf("YES\n");
	    else if(a>b && (a-b <= d))
	        printf("YES\n");
	    else if(b>a && b-a <= c)
	        printf("YES\n");
	    else 
	        printf("NO\n");
	}
	return 0;
}

