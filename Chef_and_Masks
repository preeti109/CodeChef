#include <stdio.h>

int main(void) {
	// your code goes here
	int t;
	scanf("%d", &t);
	while(t--){
	    int x, y;
	    scanf("%d %d", &x, &y);
	    if(x*100<y*10){
	        printf("disposable\n");
	    }else if(x*100==y*10){
	        printf("cloth\n");
	    }else{
	        printf("cloth\n");
	    }
	}
	return 0;
}

