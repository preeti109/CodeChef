#include <stdio.h>
int main(void) {
    int t, n, arr[4], sum, i;
	scanf("%d", &t);
    while(t--){
    scanf("%d", &n);
    for(int i=0; i<4; i++){
      scanf("%d", &arr[i]);
      sum = arr[0];
    }
      for(i=0; i<4; i++){
        if(arr[i]>sum)
        sum = arr[i];
      }
      printf("%d\n", sum);
  }
	return 0;
}
