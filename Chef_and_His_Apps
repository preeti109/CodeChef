#include <stdio.h>

int main(void) {
	// your code goes here
	int t, s, x, y, z;
	scanf("%d", &t);
	while(t--){
	    scanf("%d %d %d %d", &s, &x, &y, &z);
	    if(x+y+z>s)
	      if(x+z<=s || y+z<=s)
	      printf("1\n");
	      else
	      printf("2\n");
	    else
	    printf("0\n");
	}
	return 0;
}

